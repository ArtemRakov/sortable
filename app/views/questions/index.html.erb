<div class="container">
  <div class="col-md-10 col-md-offset-1">
    <h2> Questions </h2>
    <div id="questions" class="list-group" data-url="<%= sort_questions_path %>">
      <% @questions.each do |question| %>
        <%= link_to "#{question.id} #{question.content}", question, class: "list-group-item", id: dom_id(question) %>
      <% end %>
    </div>
  </div>
</div>
